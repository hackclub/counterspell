diff --git a/components/Minneapolis/ScrollContext.js b/components/Minneapolis/ScrollContext.js
new file mode 100644
index 0000000..b13acd2
--- /dev/null
+++ b/components/Minneapolis/ScrollContext.js
@@ -0,0 +1,17 @@
+import React, { createContext, useContext, useRef } from "react";
+
+const ScrollContext = createContext();
+
+export function ScrollProvider({ children }) {
+  const registerSectionRef = useRef(null);
+
+  return (
+    <ScrollContext.Provider value={registerSectionRef}>
+      {children}
+    </ScrollContext.Provider>
+  );
+}
+
+export function useScrollContext() {
+  return useContext(ScrollContext);
+}
